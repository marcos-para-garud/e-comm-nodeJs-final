{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/api/orders/(.*)", "dest": "/server.js" },
      { "src": "/api/users/(.*)", "dest": "/server.js" },
      { "src": "/api/products/(.*)", "dest": "/server.js" },
      { "src": "/api/cartItems/(.*)", "dest": "/server.js" }
    ]
  }